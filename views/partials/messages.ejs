    <div class="alert error">
      <%= error.msg %>
    </div>
    <% }); %> <% } %> <% if(success_msg != ''){ %>
    <div class="alert success">
      <%= success_msg %>
    </div>
    <% } %> <% if(error_msg != ''){ %>
